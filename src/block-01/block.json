{
	"$schema" : "https://schemas.wp.org/trunk/block.json",
	"apiVersion" : 3,
	"name" : "storepress/block-01",
	"version" : "0.1.0",
	"title" : "Base Block",
	"category" : "storepress",
	"icon" : "sticky",
	"description" : "StorePress Base Block",
	"attributes" : {
		"x" : {
			"type" : "number",
			"default" : 100
		},
		"y" : {
			"type" : "number",
			"default" : 100
		},
		"id" : {
			"type" : "integer",
			"default" : 0
		},
		"parent" : {
			"enum" : [
				"popup",
				"slider"
			],
			"default" : "popup"
		},
		"tooltipPosition" : {
			"type" : "string",
			"default" : "bottom"
		},
		"tooltipWidth" : {
			"type" : "number",
			"default" : 300
		},
		"isOpenedDefault" : {
			"type" : "boolean",
			"default" : false
		}
	},
	"supports" : {
		"html" : false
	},
	"textdomain" : "storepress-base-plugin",
	"editorScript" : "file:./index.js",
	"editorStyle" : "file:./index.css",
	"style" : "file:./style-index.css",
	"viewScript" : "file:./view.js",
	"render" : "file:./render.php"
}
